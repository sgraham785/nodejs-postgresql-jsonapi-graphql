{
  "name": "node-postgres-jsonapi-server",
  "version": "0.4.0",
  "private": false,
  "description": "an example api written using jsonapi-server",
  "homepage": "https://github.com/sgraham785/node-postgres-jsonapi-server",
  "author": {
    "name": "Sean Graham",
    "url": "https://github.com/sgraham785"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sgraham785/node-postgres-jsonapi-server.git"
  },
  "bugs": {
    "url": "https://github.com/sgraham785/node-postgres-jsonapi-server/issues"
  },
  "license": "MIT",
  "scripts": {
    "serve": "forever ./bin/www",
    "start": "concurrently \"npm run develop\" \"npm run watch-build\"",
    "prestart": "npm run build",
    "develop": "NODE_ENV=development forever -w ./bin/www",
    "watch-build": "babel ./src -w -d ./dist",
    "build": "babel ./src -d ./dist",
    "prebuild": "rm -rf dist/*",
    "test": "npm run lint && mocha -R spec && standard",
    "lint": "jshint src",
    "nsp": "nsp check",
    "standardjs": "standard --fix",
    "rmrf-npmi": "rm -rf node_modules && npm i"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "dotenv": "^2.0.0",
    "email-templates": "^2.5.4",
    "forever": "^0.15.3",
    "jsonapi-server": "^2.3.1",
    "jsonapi-store-relationaldb": "^1.2.8",
    "jsonwebtoken": "^7.1.9",
    "morgan": "^1.7.0",
    "nodemailer": "^2.1.0",
    "pug": "^2.0.0-beta9",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "concurrently": "^3.3.0",
    "faker": "^3.0.1",
    "jshint": "^2.5.11",
    "mocha": "^3.2.0",
    "nsp": "^2.3.0",
    "standard": "^8.6.0"
  }
}
