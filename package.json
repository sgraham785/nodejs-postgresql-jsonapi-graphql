{
  "name": "node-postgres-jsonapi-server",
  "version": "0.4.0",
  "private": false,
  "description": "an example api written using jsonapi-server",
  "homepage": "https://github.com/sgraham785/node-postgres-jsonapi-server",
  "author": {
    "name": "Sean Graham",
    "url": "https://github.com/sgraham785"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sgraham785/node-postgres-jsonapi-server.git"
  },
  "bugs": {
    "url": "https://github.com/sgraham785/node-postgres-jsonapi-server/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/www",
    "build": "babel ./src -d ./dist",
    "prebuild": "rm -rf dist/*",
    "dev": "NODE_ENV=development node ./bin/www",
    "faker": "babel-node database/seeds/faker",
    "migrate": "flyway -c dist/configs/flyway.js migrate",
    "test": "npm run lint && mocha --reporter spec --bail --check-leaks --compilers js:babel-core/register ./test/**/*.js && standard",
    "lint": "jshint src",
    "nsp": "nsp check",
    "standardjs": "standard --fix",
    "rmrf-npmi": "rm -rf node_modules && npm i"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "dotenv": "^2.0.0",
    "email-templates": "^2.5.4",
    "forever": "^0.15.3",
    "forever-monitor": "^1.7.1",
    "jsonapi-server": "^2.3.1",
    "jsonapi-store-relationaldb": "^1.2.8",
    "jsonwebtoken": "^7.1.9",
    "morgan": "^1.7.0",
    "node-flywaydb": "^0.1.4",
    "nodemailer": "^2.1.0",
    "pug": "^2.0.0-beta9",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-html-email": "^1.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chance": "^1.0.6",
    "concurrently": "^3.3.0",
    "jshint": "^2.5.11",
    "json-sql": "^0.3.10",
    "mocha": "^3.2.0",
    "nsp": "^2.3.0",
    "standard": "^9.0.2"
  },
  "babel": {
    "only": "/",
    "presets": [
      "env",
      "es2015"
    ],
    "ignore": [
      "docker/*",
      "database/*",
      "node_modules/**/*",
      "target/*",
      "logs",
      "sslcert"
    ]
  }
}
